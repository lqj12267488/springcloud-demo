<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wintop.springcloudgateway.init.dao.TokenDao">
    <select id="selectToken" parameterType="java.lang.String" resultType="com.wintop.springcloudgateway.entity.TokenBean">
        select * from user_token where token = #{token}
    </select>
</mapper>